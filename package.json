{
  "name": "@julr/unocss-preset-forms",
  "version": "0.0.3",
  "packageManager": "pnpm@7.9.5",
  "description": "ðŸ“‹ Port of @tailwindcss/forms for UnoCSS.",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Julien-R44",
  "homepage": "https://github.com/Julien-R44/unocss-preset-forms#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/unocss-preset-forms.git"
  },
  "bugs": {
    "url": "https://github.com/Julien-R44/unocss-preset-forms/issues"
  },
  "keywords": [
    "unocss",
    "@tailwind/forms",
    "tailwind forms",
    "preset uno"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "node --loader=ts-node/esm src/index.ts",
    "test": "echo OK",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "unocss": ">= 0.31.13 < 1"
  },
  "dependencies": {
    "mini-svg-data-uri": "^1.4.4"
  },
  "devDependencies": {
    "@antfu/ni": "^0.19.0",
    "@japa/assert": "^1.3.7",
    "@japa/runner": "^2.2.3",
    "@japa/spec-reporter": "^1.3.3",
    "@julr/eslint-config": "^0.3.2",
    "@types/node": "^18.13.0",
    "@unocss/preset-mini": "^0.50.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.33.0",
    "pnpm": "^7.27.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unbuild": "^0.8.10",
    "unocss": "^0.49.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "openapi-types",
        "@babel/core",
        "vite"
      ]
    }
  },
  "eslintConfig": {
    "extends": "@julr",
    "rules": {
      "n/no-unsupported-features/es-syntax": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  }
}
