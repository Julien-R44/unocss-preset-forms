{
  "name": "@julr/unocss-preset-forms",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.10.3",
  "description": "ðŸ“‹ Port of @tailwindcss/forms for UnoCSS.",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Julien-R44",
  "homepage": "https://github.com/Julien-R44/unocss-preset-forms#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/unocss-preset-forms.git"
  },
  "bugs": {
    "url": "https://github.com/Julien-R44/unocss-preset-forms/issues"
  },
  "keywords": [
    "unocss",
    "@tailwind/forms",
    "tailwind forms",
    "preset uno"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "tsx bin/test.ts",
    "quick:test": "pnpm test --",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "unocss": "^0.31.0 || ^65.0.0"
  },
  "devDependencies": {
    "@antfu/ni": "^23.3.1",
    "@japa/assert": "^4.0.1",
    "@japa/runner": "^4.2.0",
    "@japa/snapshot": "^2.0.8",
    "@julr/tooling-configs": "^3.2.0",
    "@types/node": "^22.13.1",
    "@unocss/preset-mini": "^65.4.3",
    "bumpp": "^10.0.1",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "unocss": "^65.4.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "openapi-types",
        "@babel/core",
        "@japa/core",
        "vite"
      ]
    }
  },
  "prettier": "@julr/tooling-configs/prettier"
}
